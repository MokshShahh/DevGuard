{
  "manifest_version": 3,
  "name": "DevGaurd",
  "version": "1.0",
  "description": "Make yourself more productive",
  "permissions": [
    "declarativeNetRequest","declarativeNetRequestFeedback","storage","tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action":{
    "default_popup" : "frontend/index.html"
  },
  "background": {
    "service_worker": "scripts/background.js",
    "type":"module"
  }
}