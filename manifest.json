{
  "manifest_version": 3,
  "name": "DevGaurd",
  "version": "1.0",
  "description": "Make yourself more productive",
  "permissions": [
    "declarativeNetRequest","declarativeNetRequestFeedback","storage","tabs"
  ],
  "icons": {
    "16": "./images/icon16.png",
    "32": "./images/icon32.png",
    "48": "./images/icon48.png",
    "128": "./images/icon128.png"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "action":{
    "default_popup" : "frontend/index.html"
  },
  "background": {
    "service_worker": "scripts/background.js",
    "type":"module"
  }
}