{
  "manifest_version": 3,
  "name": "Conditional Redirect",
  "version": "1.0",
  "description": "Redirect certain pages based on conditions.",
  "permissions": [
    "declarativeNetRequest","declarativeNetRequestFeedback"],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts":[{
    "matches":["https://leetcode.com/problemset/*"],
    "js":["scripts/content.js"]

  }],
  
  "background": {
    "service_worker": "scripts/background.js" 
  },
  "declarative_net_request" : {
    "rule_resources" : [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules_1.json"
  }]
  }
}
