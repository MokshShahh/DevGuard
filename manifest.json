{
  "manifest_version": 3,
  "name": "DevGaurd",
  "version": "1.0",
  "description": "Make yourself more productive",
  "permissions": [
    "declarativeNetRequest","declarativeNetRequestFeedback","storage","tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts":[{
    "matches":["https://leetcode.com/problemset/*"],
    "js":["scripts/content.js"]

  }],
  
  "background": {
    "service_worker": "scripts/background.js" 
  },
  "declarative_net_request" : {
    "rule_resources" : [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules_1.json"
  }]
  }
}
